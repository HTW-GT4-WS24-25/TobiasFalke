    padding-top: 0;
}

.unity-tab__header-label {
    background-image: none;
    -unity-slice-left: 6;
    -unity-slice-top: 6;
    -unity-slice-right: 6;
    -unity-slice-bottom: 0;
    background-color: rgba(0, 0, 0, 0);
    background-size: 100% 100%;
